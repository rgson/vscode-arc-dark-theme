{
	"$schema": "vscode://schemas/color-theme",
	"name": "Arc Dark",
	"colors": {
		"activityBar.background": "#2F343F",
		"activityBarBadge.background": "#5294E2",
		"button.background": "#383C4A",
		"button.foreground": "#FFF",
		"editor.background": "#383C4A",
		"editor.foreground": "#D3DAE3",
		"editor.inactiveSelectionBackground": "#FFF2",
		"editor.lineHighlightBackground": "#FFF1",
		"editor.selectionBackground": "#88F5",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"editorCursor.foreground": "#5294E2",
		"editorGroup.background": "#262B33",
		"editorGroup.dropBackground": "#FFFFFF0F",
		"editorGroupHeader.noTabsBackground": "#383C4A",
		"editorGroupHeader.tabsBackground": "#383C4A",
		"editorHoverWidget.background": "#383C4A",
		"editorHoverWidget.border": "#0004",
		"editorLineNumber.foreground": "#FFF4",
		"editorWhitespace.foreground": "#FFF4",
		"editorWidget.background": "#383C4A",
		"focusBorder": "#404552",
		"foreground": "#D3DAE3",
		"input.background": "#393F4C",
		"input.border": "#F5F6F7",
		"input.foreground": "#FFF",
		"inputValidation.errorBackground": "#E15158",
		"inputValidation.infoBackground": "#51B8E1",
		"inputValidation.warningBackground": "#E1AF51",
		"list.dropBackground": "#383B3D",
		"list.hoverBackground": "#0000",
		"panelTitle.activeBorder": "#383C4A",
		"sideBar.background": "#2F343F",
		"sideBarSectionHeader.background": "#2F343F",
		"sideBarTitle.foreground": "#A9ACB0",
		"statusBar.background": "#2F343F",
		"statusBar.debuggingBackground": "#E15158",
		"statusBar.foreground": "#CCC",
		"statusBar.noFolderBackground": "#2F343F",
		"statusBarItem.activeBackground": "#0004",
		"statusBarItem.hoverBackground": "#0002",
		"statusBarItem.prominentBackground": "#0044",
		"statusBarItem.prominentHoverBackground": "#0002",
		"tab.activeBackground": "#0000",
		"tab.activeBorder": "#5294E2",
		"tab.activeForeground": "#FFF",
		"tab.border": "#0000",
		"tab.inactiveBackground": "#383C4A",
		"tab.inactiveForeground": "#CCC",
		"titleBar.activeBackground": "#383C4A",
		"titleBar.activeForeground": "#CCC",
		"titleBar.inactiveBackground": "#383C4A",
		"titleBar.inactiveForeground": "#888"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#747c84"
			}
		},
		{
			"scope": [
				"constant",
				"string"
			],
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": [
				"variable.other.object.property",
				"entity.name.function",
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"foreground": "#BA75E8"
			}
		},
		{
			"scope": [
				"variable",
				"entity.name",
				"meta.function-call.arguments",
				"meta.function-call punctuation"
			],
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": [
				"variable.parameter.function-call",
				"meta.function-call.arguments keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#747c84"
			}
		},
		{
			"scope": "variable.other.object.property",
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#51B8E1"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#E15158"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#BA75E8"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5294E2"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#BA75E8"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A7E151"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#E15158"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": [
				"beginning.punctuation.definition.quote.markdown",
				"beginning.punctuation.definition.list.markdown",
				"punctuation.definition.heading.markdown",
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": [
				"markup.inline.raw",
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#51B8E1"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type.class",
				"storage.type.function",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": [
				"storage.type",
				"support.type",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#E1AF51"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#BA75E8"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#BA75E8"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.other.this",
				"variable.language"
			],
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#D3DAE3"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#A7E151"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#5294E2"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#51B8E1"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#D3DAE3"
			}
		}
	]
}
